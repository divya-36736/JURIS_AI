    <MultiAutoCompleteTextView
        android:id="@+id/multiAutoCompleteTextView"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:hint="Search....."
        android:background="@drawable/border"
        android:padding="12dp"
        android:textColor="@color/lightblack"
        android:textSize="16sp"
        android:gravity="center_vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"

        tools:ignore="LabelFor,MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:targetApi="p" />



    <ImageView
        android:id="@+id/stng"
        android:layout_width="152dp"
        android:layout_height="201dp"
        android:layout_marginTop="188dp"
        android:alpha="0.2"
        android:src="@drawable/stng"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:text="@string/welcome_to_juris_ai"
        android:textColor="@color/lightblack"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.293"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="56dp"
        android:text="HELLO"
        android:textColor="@color/lightblack"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="37dp" />

    <ImageButton
        android:id="@+id/add"
        android:layout_width="39dp"
        android:layout_height="29dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="24dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/mic"
        app:layout_constraintTop_toTopOf="@id/mic"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_add_circle_outline_24"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/mic"
        android:layout_width="19dp"
        android:layout_height="27dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="28dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/send"
        app:layout_constraintTop_toTopOf="@id/send"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_mic_none_24"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="28dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/multiAutoCompleteTextView"
        app:srcCompat="@drawable/baseline_send_24"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/abcd"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@color/lightblack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:layout_editor_absoluteY="37dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>